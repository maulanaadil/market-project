    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-flash": "0.0.2",
    "express-session": "^1.2.1",
    "express-validator": "^2.20.4",
    "fb": "^2.0.0",
    "forever": "^0.15.2",
    "form-data": "^2.1.2",
    "jade": "^1.11.0",
    "method-override": "^2.3.5",
    "moment": "^2.13.0",
    "mongoose": "^4.5.3",
    "morgan": "^1.7.0",
    "multer": "^1.3.0",
